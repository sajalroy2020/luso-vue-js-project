              <div>\n                    <img src=\"../../assets/images/clients/logo-2.png\" alt=\"\">\n                </div>\n                <div class=\"\">\n                    <p class=\"fs-2 fw-bold\">Clients</p>\n                    <p class=\"login\">LOGIN</p>\n                </div>\n            </section>\n            <section class=\"client-login-input-area border\">\n                <div class=\"mb-3\">\n                    <div class=\"\">\n                        <label for=\"inputEmail\" class=\"\">Email</label>\n                        <div class=\"position-relative\">\n                            <input v-model=\"form.email\" type=\"email\"  class=\"input-style border\" placeholder=\"Email\" id=\"inputEmail\">\n                            <img class=\"image-position\" src=\"../../assets/images/partners/mail.png\" alt=\"\">\n                        </div>\n                        <div class=\"email-below-text p-2\">\n                            <div class=\"align-items-center\">\n                                <img class=\"px-1\" src=\"../../assets/images/partners/error.png\" alt=\"\">\n                            </div>\n                            <p class=\"text-muted text-white px-1 mb-0\">Lorem ipsum dolor</p>\n                        </div>\n                        <p v-if=\"form.errors().has('email')\" class=\"error-message p-2\">\n                            {{ form.errors().get('email') }}\n                        </p>\n                    </div>\n                </div>\n                <div class=\"mb-1\">\n                    <label for=\"password\">Password</label>\n                    <div class=\"position-relative\">\n                        <input v-model=\"form.password\" :type=\"showPassword==false?'password':'text'\"  class=\"input-style border\" placeholder=\"Password\" id=\"input_password\">\n                        <img @click=\"toggleShowPassword\" class=\"image-position password-key-img\" src=\"../../assets/images/partners/ke