                          </li>\n                            <li class=\"nav-item\">\n                                <router-link to=\"/\" @click=\"activateMenu('create-account')\" :class=\"activeMenu=='create-account' ? 'header-create-ac-active':'header-create-ac' \">\n                                    <span>Create Account</span>\n                                </router-link>\n                            </li>\n                            <li class=\"nav-item\">\n                                <router-link to=\"/\" @click=\"activateMenu('sign-in')\" :class=\"activeMenu=='sign-in' ? 'header-sign-in-active':'header-sign-in' \">\n                                    <span>Sign In</span>\n                                </router-link>\n                            </li>\n                            <li class=\"nav-item position-relative\">\n                                <img @click=\"toggleHeaderDropdown\" class=\"header-right-img\" src=\"../assets/images/option.png\" alt=\"img\">\n                                <div v-if=\"isHeaderDropdownOpen\" class=\"header-option-drop-area\">\n                                    <div>\n                                        <router-link @click=\"toggleHeaderDropdown\" to=\"/welcome\" class=\"header-dropdown-link\">\n                                            <span class=\"header-dropdown-link-text\">About</span>\n                                        </router-link>\n                                    </div>\n                                    <div class=\"mt-1\">\n                                        <router-link @click=\"toggleHeaderDropdown\" to=\"/investment\" class=\"header-dropdown-link\">\n                                            <span class=\"header-dropdown-link-text\">Investment</span>\n                                        </router-link>\n                                    </div>\n                                    <div class=\"mt-1\">\n                                        <router-link @click=\"toggleHeaderDropdown\" to=\"/portfolio\" class=\"header-dropdown-link\">\n                                            <span class=\"header-dropdown-link-text\">Portfolio</span>\n                                        </router-link>\n                                    </div>\n                                    <div class=\"mt-1\">\n                                        <router-link @click=\"toggleHeaderDropdown\" to=\"/profile\" class=\"header-dropdown-link\">\n                                            <span class=\"header-dropdown-link-text\">Profile</span>\n                                        </router-link>\n                                    </div>\n                                </div>\n                            </li>\n                        </ul>\n                   </div> \n                </div>\n            </div>\n        </nav>\n        <!-- header end -->\n\n        <div>\n            <router-view></router-view>\n        </div>\n\n        <!-- footer start -->\n        <div class=\"footer p-2\">\n            <div class=\"max-w-1100 m-auto px-3\">\n                <div class=\"row mx-0\">\n                    <div class=\"col-sm-12 col-md-12 col-lg-3 p-0\">\n                        <div>\n                            <img class=\"footer-logo\" src=\"../assets/images/logo2.png\" alt=\"img\">\n                        </div>\n                    </div>\n                    <div class=\"col-sm-12 col-md-12 col-lg-6 p-0 text-center\">\n                        <div class=\"footer-link-area\">\n                            <router-link to=\"/\" class=\"footer-link\">\n                                <span>Privacy Policy</span>\n                            </router-link>\n                            <router-link to=\"/\" class=\"footer-link\">\n                                <span>Terms & Conditions</span>\n                            </router-link>\n                            <router-link to=\"/\" class=\"footer-link\">\n                            