mplate>\n    <div class=\"client-registration-page px-2\">\n        <div class=\"cr-main-content\">\n            <section class=\"d-flex justify-content-between py-4\">\n                <div>\n                    <img src=\"../../assets/images/clients/logo-2.png\" alt=\"\">\n                </div>\n                <div>\n                    <p class=\"fs-2 fw-bold\">Clients</p>\n                    <p class=\"register\">REGISTER</p>\n                </div>\n            </section>\n            <section class=\"input-area-3 border shadow\">\n                <div class=\"mb-3\">\n                    <label for=\"name\">NAME</label>\n                    <div class=\"position-relative\">\n                        <input v-model=\"form.name\" type=\"text\"  class=\"form-input-3 border\" placeholder=\"Name\" id=\"name\">\n                        <img class=\"form-input-img\" src=\"../../assets/images/partners/user.png\" alt=\"\">\n                    </div>\n                    <p v-if=\"form.errors().has('name')\" class=\"error-message p-2\">\n                        {{ form.errors().get('name') }}\n                    </p>\n                </div>\n                <div class=\"mb-3\">\n                    <label for=\"last_name\">LAST NAME</label>\n                    <div class=\"position-relative\">\n                        <input v-model=\"form.last_name\" type=\"text\"  class=\"form-input-3 border\" placeholder=\"Last name\" id=\"last_name\">\n                        <img class=\"form-input-img\" src=\"../../assets/images/partners/user.png\" alt=\"\">\n                    </div>\n                    <p v-if=\"form.errors().has('last_name')\" class=\"error-message p-2\">\n                        {{ form.errors().get('last_name') }}\n                    </p>\n                </div>\n                <div class=\"mb-3\">\n                    <label for=\"email\">EMAIL</label>\n                    <div class=\"position-relative\">\n                        <input v-model=\"form.email\" type=\"text\"  class=\"form-input-3 border\" placeholder=\"Email\" id=\"email\">\n                        <img class=\"form-input-img\" src=\"../../assets/images/partners/mail.png\" alt=\"\">\n                    </div>\n                    <p v-if=\"form.errors().has('email')\" class=\"error-message p-2\">\n                        {{ form.errors().get('email') }}\n                    </p>\n                </div>\n                <div class=\"mb-3\">\n                    <label for=\"password\">PASSWORD</label>\n                    <div class=\"position-relative\">\n                        <input v-model=\"form.password\" :type=\"showPassword==false?'password':'text'\"  class=\"form-input-3 border\" placeholder=\"Password\" id=\"input_password\">\n                        <img @click=\"toggleShowPassword\" class=\"form-input-img password-key-img\" src=\"../../assets/images/partners/key.png\" alt=\"\">\n                    </div>\n                    <p v-if=\"form.errors().has('password')\" class=\"error-message p-2\">\n                        {{ form.errors().get('password') }}\n                    </p>\n                </div>\n                <div class=\"mb-3\">\n                    <label for=\"repeat_password\">REPEAT PASSWORD</label>\n                    <div class=\"position-relative\">\n                        <input v-model=\"form.repeat_password\" :type=\"showPassword==false?'password':'text'\"  class=\"form-input-3 border\" placeholder=\"Password\" id=\"repeat_password\">\n                        <img @click=\"toggleShowPassword\" class=\"form-input-img password-key-img\" src=\"../../assets/images/partners/key.png\" alt=\"\">\n                    </div>\n                    <p v-if=\"form.errors().has('repeat_password')\" class=\"error-message p-2\">\n                        {{ form.errors().get('repeat_password') }}\n                    </p>\n                </div>\n                <div class=\"mb-3 my-4\">\n                    <div class=\"d-flex text-white\">\n                        <input v-model=\"form.status\" type=\"checkbox\" :class=\"checked?'checkbox-checked':'checkbox'\" name=\"checkbox\" id=\"checkbox\">\n                        <p class=\"px-2 d-flex text-black\">\n                            I Accept\n                            <router-link :to=\"{ path: '#' }\">\n                                <u class=\"px-1\"> Terms and Conditions</u>\n                            </router-link>\n                        </p>\n                    </div>\n                </div>\n                <div class=\"mb-3 mt-4\">\n                    <div class=\"text-center\">\n                        <button @click=\"submit\" type=\"button\" class=\"btn mt-4 text-white rounded-pill cr-register-btn\">Register</button>\n                    </div>\n                </div>\n            </section>\n            <section class=\"py-4\">\n                <div class=\"text-center pt-2\">\n                    <p class=\"d-inline-flex\">\n                        Nao estas cadastrado? \n                        <router-link :to=\"{ path: '/client/login' }\">\n                            <u class=\"px-1 special-text\"> Login </u>\n                        </router-link>\n                    </p>\n                </div>\n            </section>\n        </div>\n        <div class=\"whats-app\">\n            <img src=\"../../assets/images/partners/whatsapp.png\" alt=\"\">\n        </div>\n    </div>\n</template>\n\n<script>\nimport form from 'vuejs-form';\n\nlet loginData={\n    email:'',\n    password:'',\n};\n\nexport default {\n  name: 'login',\n  \n  data(){\n    return{\n        showPassword:false,\n        form: form.default(loginData).rules({\n            email: 'required',\n            password: 'required',\n        }).messages({\n            'email.required': 'This field is required.',\n            'password.required': 'This field is required.',\n        }),\n    }\n  },\n  methods:{\n      submit(){\n          this.$router.push({ name: 'client-home'});\n      },\n      toggleShowPassword(){\n          if(this.showPassword){\n              this.showPassword=false;\n          }\n          else{\n              this.showPassword=true;\n          }\n      },\n  },\n  mounted(){\n    \n  }\n}\n</script>\n<style>\n.client-registration-page {\n    display: grid;\n    align-content: center;\n}\n\n.whats-app{\n    position: absolute;\n    bottom: 0px;\n    right: 8px;\n    cursor: pointer;\n}\n.partners {\n    font-weight: bold;\n    font-size: 32px;\n    line-height: 48px;\n}\n.register {\n    font-size: 16px;\n    line-height: 24px;\n    text-align: right;\n    color: #5BD88D;\n    letter-spacing: 0.25em;\n    margin: 0;\n}\n.input-area-3 {\n    border-radius: 20px;\n    padding: 50px;\n}\n.form-input-3 {\n    width: 100%;\n    background-color: #FAFAFA;\n    border-radius: 8px !important;\n    color: rgb(136, 133, 133) !important;\n    padding: 12px !important;\n}\n.form-input-img{\n    position: absolute;\n    top: 13px;\n    right: 13px;\n}\n\n.special-text {\n    color: #5BD88D;\n}\n.password-key-img{\n    cursor: pointer;\n}\n.checkbox{\n    width: 20px;\n    height: 20px;\n    background-color:#4273DE !important;\n}\n.checkbox-checked {\n    background-color:#4273DE !important;\n    color: #fff;\n    width: 20px;\n    height: 20px;\n }\n</style>\n   "],"sourceRoot":""}]}