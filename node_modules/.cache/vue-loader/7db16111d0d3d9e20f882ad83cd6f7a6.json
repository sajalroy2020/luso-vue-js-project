pan>\n                            <span @click=\"activateMenu('dashboard')\">\n                                <p :class=\"activeMenu=='dashboard' ? 'left-sbar-route-active':'left-sbar-link'\">Dashboard</p>\n                            </span>\n                            <span @click=\"activateMenu('gallery')\">\n                                <p :class=\"activeMenu=='gallery' ? 'left-sbar-route-active':'left-sbar-link'\">Gallery</p>\n                            </span>\n                            <span @click=\"activateMenu('logout')\">\n                                <p :class=\"activeMenu=='logout' ? 'left-sbar-route-active':'left-sbar-link'\">Log out</p>\n                            </span>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"main-content-section mx-2\">\n                    <router-view></router-view>\n                </div>\n            </div>\n        \n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'Home',\n    components: { },\n    data(){\n        return{\n            activeMenu:'home',\n            mobileDropdownOpen: false,\n        }\n    },\n    methods:{\n        activateMenu(menu){\n            this.activeMenu = menu;\n            if(this.activeMenu === 'home'){\n                this.$router.push({ name: 'client-home' });\n            }\n            if(this.activeMenu === 'dashboard'){\n                this.$router.push({ name: 'client-dashboard' });\n            }\n            if(this.activeMenu === 'profile'){\n                this.$router.push({ name: 'client-profile' });\n            }\n            if(this.activeMenu === 'logout'){\n                // call to a method\n            }\n            return this.activeMenu;\n        },\n        toggleMobileDropdown(){\n            if(this.mobileDropdownOpen){\n                this.mobileDropdownOpen = false;\n            } else {\n                this.mobileDropdownOpen = true;\n            }\n        }\n    },\n    mounted(){\n    }\n}\n</script>\n<style scoped>\n\n\n.row {\n    margin: 0 !important;\n}\n.left-sbar-partners {\n    border: 2px solid #5BD88D;\n    color: white;\n    text-align: center;\n    padding: 2px 12px;\n    border-radius: 20px;\n    margin: 0px 18px 0px 0px;\n}\n.logo {\n    width: 120px;\n}\n.sbar-tittle {\n    color: #5BD88D;\n    letter-spacing: 0.25em;\n}\n.left-sbar-link {\n    color: #868CA0;\n    padding: 10px 15px;\n    margin: 0;\n    border-radius: 10px;\n    cursor: pointer;\n}\n.left-sbar-link:hover {\n    color: white;\n    background-color: #7B61FF;\n}\n.left-sbar-route-active {\n    color: white;\n    background-color: #7B61FF;\n    padding: 10px 15px;\n    margin: 0;\n    border-radius: 10px;\n    cursor: pointer;\n}\n</style>\n"]}]}